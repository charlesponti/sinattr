    <% @tweets.results.map do |tweet| %>
        <div class='tweet'>
          <img src="<%= tweet.user.profile_image_url %>" />
          <div class='tweet_stuff'>
            <a class='tweet_user' href="http://www.twitter.com/<%= tweet.user.screen_name %>">
              <%= tweet.user.screen_name %> </a> | <%= tweet.created_at %>
            </span><br/>
            <span class='tweet_text'><%= tweet.text %></span><br/>
          </div>
        </div>
    <% end %>